{
  "backend": {
    "email|sourceEmail": {
      "displayName": "The source email that will send contact us mails",
      "type": "string",
      "required": true,
      "example": "testmail@gmail.com"
    },
    "email|destinationEmails": {
      "displayName": "The destination emails that will receive contact us mails",
      "type": "string",
      "required": true,
      "example": "testmail@gmail.com,anothertestmail@gmail.com"
    },
    "reCaptchaSecret": {
      "displayName": "ReCaptcha secret key",
      "type": "string",
      "required": true,
      "example": "lbcLXanZS2QHLrxmz0jaVipyM5JSHt0wpXxMO8uv"
    }
  },
  "frontend": {
    "atm|host": {
      "displayName": "ATM host",
      "type": "string",
      "required": true,
      "example": "http://deep.dev.public.cdi50b11.s3-website-us-east-1.amazonaws.com",
      "default": "http://localhost:8000"
    },
    "captchaSiteKey": {
      "displayName": "Captcha site key for reCaptcha",
      "type": "string",
      "required": true,
      "example": "lbcLXanZS2QHLrxmz0jaVipyM5JSHt0wpXxMO8uv"
    }
  },
  "globals": {
      "sourceEmail": "testmail@gmail.com",
      "destinationEmails": "testmail@gmail.com,anothertestmail@gmail.com",
      "reCaptchaSecret": "TestTest1111reCaptchaSecret",
      "atm": "http://deep.dev.public.test.s3-website-us-east-1.amazonaws.com",
      "captchaSiteKey": "TestTest1111reCaptchaSiteKey"
  }
}
